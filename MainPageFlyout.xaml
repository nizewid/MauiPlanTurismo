<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiPlanTurismo.MainPageFlyout"
             xmlns:local="clr-namespace:MauiPlanTurismo">

    <FlyoutPage.Flyout>
        <local:MainPageFlyoutMenu x:Name="flyoutPage" />
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:MainPageFlyoutDetail />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>
